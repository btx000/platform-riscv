{
  "build": {
    "f_cpu": "320000000L",
    "hwids": [
      [
        "0x0403",
        "0x6010"
      ]
    ],
    "ldscript": "flash.lds",
    "mabi": "ilp32",
    "march": "rv32imac",
    "mcmodel": "medany",
    "mcu": "fe310"
  },
  "debug": {
    "tools": {
      "ftdi": {
        "init_cmds": [
          "target remote $DEBUG_PORT",
          "set arch riscv:rv32",
          "set mem inaccessible-by-default off",
          "file \"$PROG_PATH\"",
          "$INIT_BREAK",
          "$LOAD_CMD",
          "monitor init"
        ],
        "onboard": true,
        "server": {
          "arguments": [
            "-f",
            "scripts/board/sifive-hifive1.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        }
      }
    }
  },
  "frameworks": [
    "freedom-e-sdk"
  ],
  "name": "HiFive1",
  "upload": {
    "maximum_ram_size": 16384,
    "maximum_size": 16777216,
    "protocol": "ftdi"
  },
  "url": "https://www.sifive.com/products/hifive1/",
  "vendor": "SiFive"
}
