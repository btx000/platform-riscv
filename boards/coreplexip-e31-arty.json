{
  "build": {
    "f_cpu": "320000000L",
    "hwids": [
      [
        "0x0403",
        "0x6010"
      ]
    ],
    "ldscript": "flash.lds",
    "mabi": "ilp32",
    "march": "rv32imac",
    "mcmodel": "medany",
    "mcu": "e31"
  },
  "debug": {
    "tools": {
      "olimex-arm-usb-tiny-h": {
        "init_cmds": [
          "define pio_reset_halt_target",
          "   monitor init",
          "end",
          "define pio_reset_target",
          "   pio_reset_halt_target",
          "end",
          "target remote $DEBUG_PORT",
          "set arch riscv:rv32",
          "set mem inaccessible-by-default off",
          "$INIT_BREAK",
          "$LOAD_CMD",
          "pio_reset_halt_target"
        ],
        "server": {
          "arguments": [
            "-c", "adapter_khz 5000",
            "-f", "scripts/interface/ftdi/olimex-arm-usb-tiny-h.cfg",
            "-f", "scripts/board/sifive-e31arty.cfg"
          ],
          "executable": "bin/openocd",
          "package": "tool-openocd"
        }
      }
    }
  },
  "frameworks": [
    "freedom-e-sdk"
  ],
  "name": "Freedom E310 Arty (Artix-7) FPGA Dev Kit",
  "upload": {
    "maximum_ram_size": 268435456,
    "maximum_size": 16777216,
    "protocol": "olimex-arm-usb-tiny-h"
  },
  "url": "http://www.xilinx.com/products/boards-and-kits/arty.html",
  "vendor": "Xilinx"
}
